GMAT MMS1.SPADDragInterpolationMethod = Bilinear;
GMAT MMS1.SPADDragScaleFactorSigma = 1e+70;
GMAT MMS1.ModelFile = 'aura.3ds';
GMAT MMS1.ModelOffsetX = 0;
GMAT MMS1.ModelOffsetY = 0;
GMAT MMS1.ModelOffsetZ = 0;
GMAT MMS1.ModelRotationX = 0;
